{
    "name": "deluxetech/laravel-repository",
    "type": "library",
    "description": "Repository design pattern implementation for Laravel framework.",
    "keywords": [
        "repository",
        "laravel",
        "data"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Tigran Koshetsyan",
            "email": "koshetsyan.tigran@gmail.com",
            "homepage": "https://github.com/koshetsyan"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "laravel/framework": "^7.0|^8.0|^9.0",
        "reedware/laravel-relation-joins": "^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6",
        "phpcompatibility/php-compatibility": "*",
        "phpunit/phpunit": "^9",
        "squizlabs/php_codesniffer": "3.*"
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Deluxetech\\LaRepo\\ServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Deluxetech\\LaRepo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Deluxetech\\LaRepo\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-install-cmd": "\"vendor/bin/phpcs\" --config-set installed_paths vendor/phpcompatibility/php-compatibility",
        "post-update-cmd" : "\"vendor/bin/phpcs\" --config-set installed_paths vendor/phpcompatibility/php-compatibility",
        "post-autoload-dump": [
            "@php ./vendor/bin/testbench package:discover --ansi"
        ],
        "psr-12-check": [
            "phpcs --standard=PSR12 src"
        ]
    }
}
